{
  "name": "nanotestcms",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 3303 --turbo",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev -p 3303",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "db:sync": "node scripts/sync_db.js",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start -p 3301"
  },
  "dependencies": {
    "@heroui/react": "^2.7.8",
    "@payloadcms/db-mongodb": "^3.31.0",
    "@payloadcms/email-nodemailer": "^3.31.0",
    "@payloadcms/live-preview-react": "^3.31.0",
    "@payloadcms/next": "^3.31.0",
    "@payloadcms/richtext-lexical": "^3.31.0",
    "@phosphor-icons/core": "^2.1.1",
    "@phosphor-icons/react": "^2.1.7",
    "cross-env": "^7.0.3",
    "framer-motion": "^12.12.1",
    "graphql": "^16.11.0",
    "next": "^15.3.2",
    "next-themes": "^0.4.6",
    "nodemailer": "^6.10.1",
    "notistack": "^3.0.2",
    "payload": "^3.31.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-google-recaptcha-v3": "^1.10.1",
    "react-hook-form": "^7.56.4",
    "react-slick": "^0.30.3",
    "sharp": "^0.33.5",
    "slick-carousel": "^1.8.1",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.15.18",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@types/react-slick": "^0.23.13",
    "autoprefixer": "^10.4.21",
    "daisyui": "^4.12.24",
    "dotenv": "^16.5.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "esbuild": "^0.25.0"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^10.0.0"
  },
  "packageManager": "pnpm@10.7.0"
}
