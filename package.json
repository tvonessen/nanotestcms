{
  "name": "nanotestcms",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 3301 --turbo",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev -p 3301",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start -p 3301"
  },
  "dependencies": {
    "@nextui-org/react": "^2.4.8",
    "@payloadcms/db-mongodb": "beta",
    "@payloadcms/next": "beta",
    "@payloadcms/plugin-cloud": "beta",
    "@payloadcms/richtext-lexical": "beta",
    "@phosphor-icons/react": "^2.1.7",
    "cross-env": "^7.0.3",
    "framer-motion": "^11.9.0",
    "graphql": "^16.9.0",
    "next": "15.0.0-canary.161",
    "next-themes": "^0.3.0",
    "notistack": "^3.0.1",
    "payload": "beta",
    "react": "19.0.0-rc-5dcb0097-20240918",
    "react-dom": "19.0.0-rc-5dcb0097-20240918",
    "react-hook-form": "^7.53.0",
    "react-slick": "^0.30.2",
    "sharp": "0.32.6",
    "slick-carousel": "^1.8.1",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "@types/react-slick": "^0.23.13",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.10",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.0-canary.160",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "5.6.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9.0.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.1",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
    }
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
  }
}
