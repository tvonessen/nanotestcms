{
  "name": "nanotestcms",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 3303 --turbo",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev -p 3303",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "db:sync": "node scripts/sync_db.js",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start -p 3301"
  },
  "dependencies": {
    "@heroui/react": "^2.8.7",
    "@payloadcms/db-mongodb": "^3.69.0",
    "@payloadcms/email-nodemailer": "^3.69.0",
    "@payloadcms/live-preview-react": "^3.69.0",
    "@payloadcms/next": "^3.69.0",
    "@payloadcms/richtext-lexical": "^3.69.0",
    "@phosphor-icons/core": "^2.1.1",
    "@phosphor-icons/react": "2.1.10",
    "cross-env": "^10.1.0",
    "framer-motion": "^12.23.26",
    "graphql": "^16.12.0",
    "next": "^16.1.0",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.11",
    "notistack": "^3.0.2",
    "payload": "^3.68.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-google-recaptcha-v3": "^1.11.0",
    "react-hook-form": "^7.69.0",
    "react-slick": "^0.31.0",
    "sharp": "^0.34.5",
    "slick-carousel": "^1.8.1",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.19.1",
    "@types/nodemailer": "^6.4.21",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-slick": "^0.23.13",
    "autoprefixer": "^10.4.23",
    "daisyui": "^4.12.24",
    "dotenv": "^16.6.1",
    "esbuild": "^0.27.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": "^24",
    "pnpm": "^10.0.0"
  },
  "packageManager": "pnpm@10.25.0"
}
