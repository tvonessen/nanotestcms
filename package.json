{
  "name": "nanotestcms",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 3303",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev -p 3303",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "db:sync": "node scripts/sync_db.js",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start -p 3301"
  },
  "dependencies": {
    "@heroui/react": "2.6.14",
    "@payloadcms/db-mongodb": "3.20.0",
    "@payloadcms/email-nodemailer": "^3.20.0",
    "@payloadcms/live-preview-react": "^3.20.0",
    "@payloadcms/next": "3.20.0",
    "@payloadcms/plugin-form-builder": "^3.20.0",
    "@payloadcms/richtext-lexical": "3.20.0",
    "@phosphor-icons/core": "^2.1.1",
    "@phosphor-icons/react": "2.1.7",
    "cross-env": "^7.0.3",
    "framer-motion": "^12.0.6",
    "graphql": "^16.10.0",
    "next": "15.1.6",
    "next-themes": "^0.4.4",
    "nodemailer": "^6.10.0",
    "notistack": "^3.0.2",
    "payload": "3.20.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "react-slick": "^0.30.3",
    "sharp": "0.33.5",
    "slick-carousel": "^1.8.1",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.12.0",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@types/react-slick": "^0.23.13",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.23",
    "dotenv": "^16.4.7",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^10.0.0"
  },
  "packageManager": "pnpm@10.2.0"
}
